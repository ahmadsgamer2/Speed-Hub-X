--- Local
local DiscordInvite = "https://discord.gg/x9GkmYvPyr"

-- _G.
_G.Key_System = false
_G.Version = "Version: 2.2.7"
_G.Discord = DiscordInvite
-- Getgenv
getgenv().Discord = DiscordInvite

-- Anti Lag

if getgenv().AntiLag then
	local renderStepped = game:GetService("RunService").RenderStepped
	local function GetFPS() 
		renderStepped:Wait() 
		local V = 0 for _ = 1, 5 do V += 1 / renderStepped:Wait() 
		end return math.round(V / 5) 
	end
     spawn(function()setfpscap(GetFPS())end)
end
-- PS99 List
if getgenv()["List Egg PS99"] then
    local ETable = {}
    for i,v in ReplicatedStorage.__DIRECTORY.Eggs["Zone Eggs"]:GetDescendants() do
        if v:IsA("ModuleScript") then
            table.insert(ETable, string.split(v.Name, " | ")[2])
        end
    end
end
